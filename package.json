{
  "name": "vue-runtime-template-compiler",
  "version": "1.0.0",
  "description": "A simple Vue component to compile templates at runtime",
  "main": "dist/vue-runtime-template-compiler.umd.js",
  "module": "dist/vue-runtime-template-compiler.esm.js",
  "unpkg": "dist/vue-runtime-template-compiler.umd.js",
  "browser": "dist/vue-runtime-template-compiler.esm.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test jest --runInBand",
    "start": "cross-env NODE_ENV=development webpack-dev-server --config build/webpack.dev.config.js",
    "esm": "cross-env NODE_ENV=production webpack --config build/webpack.esm.config.js",
    "umd": "cross-env NODE_ENV=production webpack --config build/webpack.umd.config.js",
    "lint": "cross-env eslint --ext .js,.vue --ignore-path .gitignore . --fix",
    "clean": "cross-env rimraf dist",
    "build": "npm run clean && npm run lint && npm test && npm run esm && npm run umd"
  },
  "author": "Jonathan Watkins",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jonwatkins/vue-runtime-template-compiler.git"
  },
  "bugs": {
    "url": "https://github.com/jonwatkins/vue-runtime-template-compiler/issues"
  },
  "homepage": "https://github.com/jonwatkins/vue-runtime-template-compiler#readme",
  "files": [
    "src",
    "dist"
  ],
  "keywords": [
    "vuejs",
    "dynamic",
    "runtime",
    "template"
  ],
  "dependencies": {
    "vue-template-compiler": "^2.6.11"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/plugin-proposal-export-default-from": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "@purtuga/esm-webpack-plugin": "^1.2.1",
    "@vue/test-utils": "^1.0.0-beta.31",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.6",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-vue": "^6.2.1",
    "faker": "^4.1.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "jest": "^25.1.0",
    "jest-serializer-vue": "^2.0.2",
    "moment": "^2.24.0",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "terser-webpack-plugin": "^2.3.5",
    "vue": "^2.6.11",
    "vue-jest": "^3.0.5",
    "vue-loader": "^15.9.0",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2"
  }
}
